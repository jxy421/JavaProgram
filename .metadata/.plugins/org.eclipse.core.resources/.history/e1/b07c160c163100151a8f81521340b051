package thread.safe.bank;
public class DrawTest { 
    public static void main(String[] args) { 
        // 创建一个账户
        Account acct = new Account("1234567", 1000.0); 
        // 模拟两个线程对同一个账户取钱
        while(true){
        	new DrawThread("甲", acct, 100).start(); 
            new DrawThread("乙", acct, 100).start(); 
        }
    } 
} 
