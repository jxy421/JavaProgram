package thread;
/**
 * 中断异常演示
 * @author sunddenly
 *
 */
public class TestInterruptedException {
	public static void main(String[] args) {
		/**
		 * 进入睡眠阻塞
		 */
		Thread t1 =new Thread(){
			public void run(){
				System.out.println("进入睡眠。。。。。");
				try{
					Thread.sleep(1000000);
				} catch(InterruptedException e){
					System.out.println("唤醒睡眠结束");
				}
			}
		};
	}
}
